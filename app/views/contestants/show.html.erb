<section id="contestant-info">
  <%= @contestant.name %>
  <%= @contestant.bachelorette.season_number %>
  <%= @contestant.bachelorette.season_description %>
</section>

<section id="outings">
  <% @contestant.outings.each do |outing| %>
    <section id="outing-<%= outing.id %>">
      <%= link_to outing.name, "/outings/#{outing.id}" %>
    </section>
  <% end %>
</section>